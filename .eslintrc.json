{
    "extends": [
        "@rocketseat/eslint-config/next",
        "next/core-web-vitals"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "all",
                "tabWidth": 4,
                "semi": true,
                "singleQuote": false,
                "bracketSpacing": true,
                "eslintIntegration": true,
                "printWidth": 70
            }
        ]
    }
}